{
  "name": "choo-choo",
  "description": "Small JavaScript library for creating railroad diagrams from Bakus-Naur Form grammars",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/estudio-hawara/choo-choo.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc --module ESNext --outDir dist/esm",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "prepare": "pnpm run build",
    "test": "jest",
    "coverage": "jest --collect-coverage"
  },
  "keywords": [
    "bnf",
    "ebnf"
  ],
  "packageManager": "pnpm@10.8.1",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3"
  },
  "author": "Carlos Capote PÃ©rez Andreu <carlos.capote@hawara.es>",
  "license": "MIT"
}
